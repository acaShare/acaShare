@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section ModeratorPanelCSS {
    <link rel="stylesheet" href="~/css/moderator-panel.css">
}

<div class="row home-container">
    <div class="col s12 m5 l3 home-part">
        <div class="data-container">
            <nav class="colornav">
                <div class="nav-wrapper path-header-wrapper">
                    <div class="col s12 tree-path">
                        Akcje
                    </div>
                </div>
            </nav>

            <div class="home-content">
                <ul class="moderator-actions">
                    <li class="moderator-actions-item">
                        <a class="moderator-actions-item-link" asp-action="">Sugestie edycji</a>
                    </li>
                    <li class="moderator-actions-item">
                        <a class="moderator-actions-item-link" asp-action="">Sugestie usunięcia</a>
                    </li>
                    <li class="moderator-actions-item">
                        <a class="moderator-actions-item-link" asp-action="">Materiały oczekujące na zatwierdzenie</a>
                    </li>
                    <li class="moderator-actions-item">
                        <a class="moderator-actions-item-link" asp-controller="StructureManagement" asp-action="Home">Zarządzaj strukturą uczelni</a>
                    </li>
                    <li class="moderator-actions-item">
                        <a class="moderator-actions-item-link" asp-action="">Zarządzaj moderatorami</a>
                    </li>
                </ul>
            </div>
        </div>
    </div>

    <div class="col s12 m7 l9 home-part">
        <div class="data-container">
            <nav class="colornav">
                <div class="nav-wrapper path-header-wrapper">
                    <div class="col s12 tree-path">
                        Panel moderatora
                    </div>
                </div>
            </nav>

            <div class="home-content">
                @RenderBody()
            </div>
        </div>
    </div>
</div>