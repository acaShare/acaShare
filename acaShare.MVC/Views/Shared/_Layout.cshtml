<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0-beta/css/materialize.min.css">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link rel="stylesheet" href="~/css/home.css">
</head>
<body>
    <nav>
        <div class="nav-wrapper">
            <a href="home.html" class="brand-logo">acaShare</a>
            <ul id="nav-mobile" class="right hide-on-med-and-down">
                <li><a href="panel_administratora.html">Panel administratora</a></li>
                <li><a href="panel_moderatora.html">Panel moderatora</a></li>
                <li><a href="login.html">Wyloguj</a></li>
            </ul>
        </div>
    </nav>

    <div class="row home-container">
        <div class="col s12 m8 l9 home-part">
            <div class="data-container">
                <nav class="colornav">
                    <div class="nav-wrapper path-header-wrapper">
                        <div class="col s12 tree-path">
                            <a href="home.html" class="breadcrumb">Uczelnie</a>
                        </div>
                    </div>
                </nav>

                <div class="home-content">
                    @RenderBody()
                </div>
            </div>
        </div>

        @await Component.InvokeAsync("Sidebar")
    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0-beta/js/materialize.min.js"></script>
    <script src="~/js/home.js"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
